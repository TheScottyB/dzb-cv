    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.15.2",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.1.2",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "handlebars": "^4.7.8",
    "happy-dom": "^17.4.6",
    "highlight.js": "^11.11.1",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "marked": "^15.0.11",
    "marked-gfm-heading-id": "^4.1.1",
    "marked-highlight": "^2.2.1",
    "marked-mangle": "^1.1.10",
    "marked-smartypants": "^1.1.9",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.2.133",
    "prettier": "^3.0.0",
    "prompts": "^2.4.2",
