{
  "name": "dzb-cv",
  "version": "1.0.0",
  "private": true,
  "description": "Profile management system for Dawn",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "volta": {
    "node": "20.10.0",
    "pnpm": "10.9.0"
  },
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=10.9.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "husky"
  },
