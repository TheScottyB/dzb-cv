{
  "name": "dzb-cv",
  "version": "1.0.0",
  "description": "Profile management system for Dawn",
  "main": "dist/cli/index.js",
  "type": "module",
  "bin": "dist/cli/index.js",
  "volta": {
    "node": "20.10.0",
    "pnpm": "10.9.0"
  },
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=10.9.0"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/cli/index.js",
    "start": "node dist/cli/index.js",
    "dev": "tsx watch src/cli/index.ts",
    "dev:debug": "tsx --inspect-brk watch src/cli/index.ts",
    "cli": "tsx src/cli/index.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "setup": "chmod +x setup.sh && ./setup.sh",
    "link-cli": "pnpm link --global",
    "score": "node src/scripts/score-resume.js",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^13.1.0",
    "debug": "^4.4.0",
    "docx4js": "^3.3.0",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.7.0",
    "inquirer": "^12.6.0",
    "jsdom": "^26.1.0",
    "mammoth": "^1.6.0",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.11",
    "marked-gfm-heading-id": "^4.1.1",
    "marked-highlight": "^2.0.1",
    "marked-mangle": "^1.1.0",
    "marked-smartypants": "^1.1.3",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.1.91",
    "prompts": "^2.4.2",
    "puppeteer": "^24.7.2",
    "supports-color": "^10.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/debug": "^4",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^20.0.0",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "ms": "^2.1.3",
    "pnpm": "10.9.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "types": "./dist/cli/index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "keywords": [],
  "author": "thescottybots",
  "license": "ISC"
}