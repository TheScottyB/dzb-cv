{
  "name": "@dzb-cv/pdf",
  "version": "1.2.0",
  "description": "PDF generation functionality for DZB CV Generator",
  "private": true,
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./generators": {
      "types": "./dist/generators/index.d.ts",
      "import": "./dist/generators/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run || true",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist coverage tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@dzb-cv/types": "workspace:*",
    "markdown-it": "^14.1.0",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.15.0"
  },
  "devDependencies": {
    "@dzb-cv/config": "workspace:*",
    "@types/markdown-it": "^13.0.9"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "pdf",
    "cv",
    "resume",
    "generator",
    "html-to-pdf",
    "markdown-to-pdf"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}