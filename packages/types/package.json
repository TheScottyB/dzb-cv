{
  "name": "@dzb-cv/types",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cv": {
      "types": "./dist/cv/index.d.ts",
      "import": "./dist/cv/index.js"
    },
    "./pdf": {
      "types": "./dist/pdf/index.d.ts",
      "import": "./dist/pdf/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist coverage"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
