{
  "name": "@dzb-cv/cli",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "cv": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run || true",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist coverage tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@dzb-cv/core": "workspace:*",
    "@dzb-cv/pdf": "workspace:*",
    "@dzb-cv/templates": "workspace:*",
    "@dzb-cv/types": "workspace:*",
    "commander": "^13.0.0",
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "@dzb-cv/config": "workspace:*"
  }
}