    "puppeteer-core": "^24.8.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "turbo": "latest",
    "typescript": "^5.0.0",
    "uuid": "^11.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@vitest/coverage-v8": "^1.2.1"
